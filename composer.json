{
    "name": "pdaleramirez/super-payment-adjuster",
    "description": "Adds ability to adjust order amount based on the payment method selected.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "super payment adjuster"
    ],
    "support": {
        "docs": "https://github.com/pdaleramirez/super-payment-adjuster/blob/master/README.md",
        "issues": "https://github.com/pdaleramirez/super-payment-adjuster/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dale Ramirez",
            "homepage": "https://github.com/pdaleramirez"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0"
    },
    "require-dev": {
      "craftcms/cms": "^4.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "pdaleramirez\\superpaymentadjuster\\": "src/"
        }
    },
    "extra": {
        "name": "Super Payment Adjuster",
        "handle": "super-payment-adjuster",
        "developer": "Dale Ramirez",
        "developerUrl": "https://github.com/pdaleramirez",
        "documentationUrl": "https://github.com/pdaleramirez/super-payment-adjuster/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/pdaleramirez/super-payment-adjuster/master/CHANGELOG.md",
        "class": "pdaleramirez\\superpaymentadjuster\\SuperPaymentAdjuster"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
